{
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "no-unused-vars": "off",
        "semi": "off",
        "indent": [
            "warn",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "no-console": "off",
        "no-prototype-builtins": "off", // enable obj.hasOwnProperty
        "no-debugger": "warn"
    }
}